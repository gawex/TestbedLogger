    <!-- DialogAboutApplication STRINGS-->
    <string name="dialog_about_application_title">Něco o téhle aplikaci</string>
    <string name="dialog_about_application_message">
                Tato aplikace slouží ke komunikaci s produktem TESTBED 1.2, který je součástí nové
                výrobní linky SmartFactory.\n\n
                Aplikace vznikla jako součást diplomová práce <b>Uživatelské aplikace proAndorid OS
                pro produkt Testbed</b> (<i>User apps for Android OS for Testbed</i>)\n\n
                Aplikace vznikala od září 2019 do června 2021\n
                Vytvořil: Tatarin Lukáš, Bc.
            </string>
    <string name="dialog_about_application_neutral_button">Tohle jsem chtěl vědět</string>

    <!-- DialogDeviceIsNotInitilaziedProperly STRINGS-->
    <string name="dialog_device_is_not_initilazied_properly_title">Tohle nepůjde</string>
    <string name="dialog_device_is_not_initilazied_properly_message">
                Tak to vypadá, že toto zařízení jsme tady již měli připojené ale nyní se hlásí jinak.\n
                Zřejmě se při jeho zapnutí špatně iniciovali senzory. Prosím <b>restartuj zařízení</b>
                a prověď <b tools:ignore="Typos">znova proces nalezení a objevení</b> zařízení.
            </string>
    <string name="dialog_device_is_not_initilazied_properly_neutral_button">Tak jo teda</string>

    <!-- DialogDeviceDoNotHaveAnySensors STRINGS-->
    <string name="dialog_device_do_not_have_any_sensors_title">Tohle nepůjde</string>
    <string name="dialog_device_do_not_have_any_sensors_message">
                Tak to vypadá, že toto zařízení nemá žádní osazené senzory, nebo se zřejmě se při
                jeho zapnutí špatně iniciovali. Prosím <b>restartuj zařízení</b>
                a prověď <b tools:ignore="Typos">znova proces nalezení a objevení</b> zařízení,
                nebo použij jiné zařízení.
            </string>
    <string name="dialog_device_do_not_have_any_sensors_button">Tak jo teda</string>

