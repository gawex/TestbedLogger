    <!-- DialogBleNotSupported STRINGS-->
    <string name="dialog_ble_not_supported_title">Nemáš Bluetooth LE</string>
    <string name="dialog_ble_not_supported_message">
                Bluetooth Low Energy, které využívá náš Testbed, není na tvém zařízení podporováno.
                Provoz aplikace na tvém zařízení není možný a nedává žádný symsl,
                proto bude aplikace ukončena.
            </string>
    <string name="dialog_ble_not_supported_negative_button">Tomu nevěřím</string>

    <!-- DialogLocationPermissionRequest STRINGS-->
    <string name="dialog_location_permission_request_title">Potřebujeme nějaké opravnění</string>
    <string name="dialog_location_permission_request_message">
                Aplikace pro Testbed využívá technologii <b>Bluetooth Low Energy</b>,která
                pro vyhledání a připojení k zařízením potřebuje opravnění k přístupu k poloze.\n\n
                Neboj, žádné údaje o tvojí poloze <b>neshomaždujeme</b> a <b>neukládáme</b>,
                jen jí potřebujeme pro správnou funkci aplikace. <b>Prosím uděl nám oprávnění.</b>
            </string>
    <string name="dialog_location_permission_request_positive_button">Ano, rozumím</string>
    <string name="dialog_location_permission_request_negative_button">Tomu nevěřím</string>

    <!-- DialogLocationPermissionDenied STRINGS-->
    <string name="dialog_location_permission_denied_title">Bez polohy to nepůjde</string>
    <string name="dialog_location_permission_denied_message">
                Tak takhle to opravdu nepůjde. Bez povolení přistupu k poloze se opravdu neobejdeme,
                proto bude aplikace ukončena.\n\n
                Pokud sis to rozmyslel, povol oprávnění k přístupu k poloze.
            </string>
    <string name="dialog_location_permission_denied_positive_button">Rozmyslel jsem si to</string>
    <string name="dialog_location_permission_denied_negative_button">Někdy příště</string>

    <!-- DialogLocationPermissionDeniedForever STRINGS-->
    <string name="dialog_location_permission_denied_forever_title">Nechtěl jsi být znova obtěžován</string>
    <string name="dialog_location_permission_denied_forever_message">
                Tak takhle to opravdu nepůjde.
                Bez povolení přístupu k poloze se opravdu neobejdeme.\n\n
                Navíc jsi zvolil možnost nebýt znova dotazem na povolení obtěžován.
                Teď to již musíš sám změnit v nastavení aplikace, nebo ji přeinstalovat.
                Ale neboj, i s tím ti pomůžeme.\n\n
                Jdi do systémového nastavení aplikace a v záložce <b>Oprávnění aplikací</b>
                povol přístup k <b>Poloze</b>. Poté aplikaci restartuj.\n\n
                <b>TIP: </b>Do systémového nastavení se dostaneš pomocí tlačítka
                <b>Systémové nastavení</b>.
            </string>
    <string name="dialog_location_permission_denied_forever_positive_button">Systémové nastavení</string>
    <string name="dialog_location_permission_denied_forever_negative_button">Tomu nevěřím</string>

    <!-- DialogLocationRequest STRINGS-->
    <string name="dialog_location_request_title">Potřebujeme zapnout polohu</string>
    <string name="dialog_location_request_message">
                Používáš <b>Android 10 a vyšší</b>, který pro vyhledání <b>Bluetooth Low Energy</b>
                zařízení a tedy i Testbed zařázení potřebuje zapnout i samotnou polohu zařízení.\n\n
                Neboj, žádné údaje o tvojí poloze <b>neshomaždujeme</b> a <b>neukládáme</b>,
                jen jí potřebujeme pro správnou funkci aplikace. <b>Prosím zapni polohu.</b>
            </string>
    <string name="dialog_location_request_positive_button">Zapnout polohu</string>
    <string name="dialog_location_request_negative_button">Tomu nevěřím</string>

