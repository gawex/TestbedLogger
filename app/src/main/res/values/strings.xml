    <!-- Dialogs Application STRINGS -->
    <!-- BLE is not supported STRINGS-->
    <string name="dialog_title_ble_not_supported">Nemáš Bluetooth LE</string>
    <string name="dialog_message_ble_not_supported">
                Bluetooth Low Energy, které využívá náš Testbed, není na tvém zařízení podporováno.
                Provoz aplikace na tvém zařízení není možný a nedává žádný symsl,
                proto bude aplikace ukončena.
            </string>
    <!-- Permission request STRINGS-->
    <string name="dialog_title_location_permission_request">Potřebujeme nějaké opravnění</string>
    <string name="dialog_message_location_permission_request">
                Aplikace pro Testbed využívá technologii <b>Bluetooth Low Energy</b>,která
                pro vyhledání a připojení k zařízením potřebuje opravnění k přístupu k poloze.\n\n
                Neboj, žádné údaje o tvojí poloze <b>neshomaždujeme</b> a <b>neukládáme</b>,
                jen jí potřebujeme pro správnou funkci aplikace. <b>Prosím uděl nám oprávnění.</b>
            </string>

    <!-- Permission denied STRINGS-->
    <string name="dialog_title_location_permission_denied">Bez polohy to nepůjde</string>
    <string name="dialog_message_location_permission_denied">
                Tak takhle to opravdu nepůjde. Bez povolení přistupu k poloze se opravdu neobejdeme,
                proto bude aplikace ukončena.\n\n
                Pokud sis to rozmyslel, povol oprávnění k přístupu k poloze.
            </string>

    <!-- Permission denied forever STRINGS-->
    <string name="dialog_title_location_permission_denied_forever">Nechtěl jsi být znova obtěžován</string>
    <string name="dialog_message_location_permission_denied_forever">
                Tak takhle to opravdu nepůjde.
                Bez povolení přístupu k poloze se opravdu neobejdeme.\n\n
                Navíc jsi zvolil možnost nebýt znova dotazem na povolení obtěžován.
                Teď to již musíš sám změnit v nastavení aplikace, nebo ji přeinstalovat.
                Ale neboj, i s tím ti pomůžeme.\n\n
                Jdi do systémového nastavení aplikace a v záložce <b>Oprávnění aplikací</b>
                povol přístup k <b>Poloze</b>. Poté aplikaci restartuj.\n\n
                <b>TIP: </b>Do systémového nastavení se dostaneš pomocí tlačítka
                <b>Systémové nastavení</b>.
            </string>

    <!-- Location adapter request STRINGS-->
    <string name="dialog_title_location_request">Potřebujeme zapnout polohu</string>
    <string name="dialog_message_location_request">
                Používáš <b>Android 10 a vyšší</b>, který pro vyhledání <b>Bluetooth Low Energy</b>
                zařízení a tedy i Testbed zařázení potřebuje zapnout i samotnou polohu zařízení.\n\n
                Neboj, žádné údaje o tvojí poloze <b>neshomaždujeme</b> a <b>neukládáme</b>,
                jen jí potřebujeme pro správnou funkci aplikace. <b>Prosím zapni polohu.</b>
            </string>

    <!-- Location adapter denied STRINGS-->
    <string name="dialog_title_location_denied">Bez zapnuté polohy to nepůjde</string>
    <string name="dialog_message_location_denied">
                Tak takhle to opravdu nepůjde. Bez zapnuté polohy se na <b>Androidu 10 a vyšším</b>
                opravdu neobejdeme, proto bude aplikace ukončena.\n\n
                Pokud sis to rozmyslel, prosím zapni polohu.
            </string>

    <!-- Bluetooth adapter denied STRINGS-->
    <string name="dialog_title_bluetooth_denied">Potřebujeme zapnout Bluetooth</string>
    <string name="dialog_message_bluetooth_denied">
                Tak takhle to opravdu nepůjde. Bez Bluetooth adaptéru se opravdu neobejdeme.\n\n
                Pokud sis to rozmyslel, zapni prosím <b>Bluetooth adaptér</b>,
                jinak bude aplikace ukončena.
            </string>

    <!-- Bluetooth adapter disabled STRINGS -->
    <string name="dialog_title_bluetooth_disabled">Nevypínej mi Bluetooth</string>
    <string name="dialog_message_bluetooth_disabled">
                Tak takhle to opravdu nepůjde. Bez Bluetooth adaptéru se opravdu neobejdeme.\n\n
                Pokud sis to rozmyslel, zapni prosím <b>Bluetooth adaptér</b> ať můžeme pokračovat.
            </string>

    <!-- Location adapter disabled STRINGS -->
    <string name="dialog_title_location_disabled">Nevypínej mi Polohu</string>
    <string name="dialog_message_location_disabled">
                Tak takhle to opravdu nepůjde. Bez zapnuté polohy se na <b>Androidu 10 a vyšším</b>
                opravdu neobejdeme.\n\n
                Pokud sis to rozmyslel, zapni prosím <b>Polohu</b> ať můžeme pokračovat ve skenování.
            </string>

    <!-- About Application STRINGS-->
    <string name="dialog_title_about_application">Něco o téhle aplikaci</string>
    <string name="dialog_message_about_application">
                Tato aplikace slouží ke komunikaci s produktem TESTBED 1.2, který je součástí nové
                výrobní linky SmartFactory.\n\n
                Aplikace vznikla jako součást diplomová práce <b>Uživatelské aplikace proAndorid OS
                pro produkt Testbed</b> (<i>User apps for Android OS for Testbed</i>)\n\n
                Aplikace vznikala od září 2019 do června 2021\n
                Vytvořil: Tatarin Lukáš, Bc.
            </string>

